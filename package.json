{
  "name": "skyvps360-platform",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "tsx server/index.ts",
    "clean": "bash ./scripts/clean-vite.sh",
    "prebuild": "bash ./fix-missing-modules.sh && node scripts/fix-schema-imports.mjs && node scripts/fix-schema-types.mjs && bash ./scripts/install-optional-deps.sh",
    "build": "npm run clean && npm run fix-schema-types && vite build --outDir dist/client && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist/server --sourcemap",
    "build:production": "cross-env NODE_ENV=production npm run build",
    "build:digitalocean": "npm run build",
    "postbuild": "bash ./scripts/fix-migrations.sh && cp .env* dist/ && bash ./scripts/ensure-client-files.sh",
    "start": "export NODE_ENV=production && cp -r migrations dist/ && node dist/server/index.js",
    "start:dev": "cross-env NODE_ENV=development node dist/server/index.js",
    "db:push": "drizzle-kit push",
    "db:push:force": "drizzle-kit push --accept-data-loss",
    "db:migrate": "node scripts/run-migrations.mjs",
    "docker:build": "docker build -t skyvps360 .",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "schema:fix-imports": "node scripts/fix-schema-imports.mjs",
    "schema:fix-types": "node scripts/fix-schema-types.mjs",
    "schema:ensure-client": "node ./scripts/ensure-client-schema.mjs",
    "check": "tsc",
    "postinstall": "if [ \"$NODE_ENV\" != \"development\" ]; then npm run db:push || true; fi"
  },
